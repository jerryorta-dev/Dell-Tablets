package com.dell.controls.components.templateMVCFactory.mvc.control {import com.dell.controls.components.templateMVCFactory.mvc.model.ITemplateModel;import com.dell.events.UIEvent;public class TemplateController implements ITemplateController {				private var model:Object;				public function TemplateController(oModel:ITemplateModel) {			this.model = oModel;		}        public function viewImputHandler( data:Object ):void {            //change data here            this.model.data = data;        }        public function viewEventHandler(event:UIEvent):void {            this.model.data = event.data;        }	}}