package com.dell.controls.components.scrubbers.mvc.view.core {import com.dell.controls.components.core.abstract.AbstractUI;import com.dell.controls.components.scrubbers.mvc.control.IScrubberController;import com.dell.controls.components.scrubbers.mvc.view.core.abstract.AbstractScrubberComponentView;import com.dell.controls.components.templateMVCFactory.mvc.view.core.abstract.AbstractTemplateComponentView;import com.dell.events.UIEvent;import flash.display.DisplayObject;
public class AbstractScrubberCompositeView extends AbstractScrubberComponentView {			protected var viewChildren:Array;				public function AbstractScrubberCompositeView( aModel:Object, aController:IScrubberController, target:DisplayObject = null) {			super(aModel, aController);			this.viewChildren = new Array();		}				override public function add(c:AbstractUI):void {			viewChildren.push(c);		}				override public function update(event:UIEvent = null):void {			for each (var c:AbstractTemplateComponentView in viewChildren) {				c.update( event );			}		}	}}